    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "copy-readme": "cp README.md libs/xng-breadcrumb/README.md",
    "copy-version": "VERSION=$npm_package_version && cd libs/xng-breadcrumb && npm version $VERSION && cd ../..",
    "commit-version": "git add . && git commit -m $npm_package_version && git push origin main",
    "prepackage": "git checkout main && git pull origin main && npm-run-all copy-* commit-version",
    "package": "yarn build && npm publish ./dist/libs/xng-breadcrumb",
    "release": "release-please release-pr --token=$GITHUB_XNG_TOKEN --repo-url=https://github.com/udayvunnam/xng-breadcrumb --release-type=node --package-name=xng-breadcrumb",
    "prepare": "husky install",
    "serve-docs": "docsify serve docs"
